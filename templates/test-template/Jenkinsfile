import groovy.io.FileType

node {
  stage('test-file-parameter') {
        def devData = readYaml file: 'templates/test-template/template.yaml'
        def serviceAccountName = devData.parameters.name
        println "Hello World. ${serviceAccountName}"
    


        def list = []

        def dir = new File("path_to_parent_dir")
        dir.eachFileRecurse (FileType.FILES) { file ->
        list << file
        list.each {
        println it.path
        }
    }
  }
}
